            <DOMFrame index="0" keyMode="9728">
              <Actionscript>
                <script><![CDATA[import flash.display.MovieClip;

var vcam:MovieClip;
var particles:MovieClip;

var timer:Number;
var timerOrig:Number;

var cTimer:Number;

init();

function init ():void {
	visible = false;
	
	vcam = MovieClip(parent).vcam;
	particles = MovieClip(parent).particleLayer;
	
	timer = 0;
	timerOrig = 80;
	
	if (cTimer) {
		timerOrig = cTimer;
	}
}

function update():void {
	
	if (CameraDistCheck.distanceBetween(this, vcam) < 800) { 
		
		if (timer > 0) {
			timer --;
		} else {
			timer = timerOrig;
			dropBomb();
		}
		
	}
	
}

function dropBomb():void {
		
	var bomb:MovieClip = particles.addBomb(x, y, 1); 
	bomb.xSpeed = 0;
}]]></script>
