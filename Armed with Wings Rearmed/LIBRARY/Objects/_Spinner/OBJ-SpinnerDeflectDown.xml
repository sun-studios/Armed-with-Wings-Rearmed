              <Actionscript>
                <script><![CDATA[import flash.display.MovieClip;

alpha = 0;

var dir:int = -1;
var spinnerObjects:Array;
//component def
var spinnerObjName:Array;

init();

function init ():void {
	spinnerObjects = new Array();
	
	for (var i:int = 0; i < spinnerObjName.length; i ++) {
		var obj:MovieClip = MovieClip(parent)[spinnerObjName[i]];
		if (obj) {
			spinnerObjects.push(obj);
		}
	}
}

function update():void {
	if (spinnerObjects.length > 0) {
		for (var i:int = 0; i < spinnerObjects.length; i ++) {
			var obj:MovieClip = spinnerObjects[i];
			
			if (this.hitTestObject(obj.bound)) {
				obj.dir = dir;
			}
			
		}
	}
}]]></script>
