              <Actionscript>
                <script><![CDATA[stop();

import flash.text.TextField;
import flash.display.MovieClip;

var score_tf:TextField;
var level_tf:TextField;
var time_tf:TextField;
var combo_tf:TextField;
var lives_tf:TextField;

var weaponIcon:MovieClip;
var gearIcon0:MovieClip;
var gearIcon1:MovieClip;
var gearIcon2:MovieClip;
var gearIcon3:MovieClip;

var gameData:MovieClip;
var bestRun:Array;

init();

function init():void {
	gameData = MovieClip(root);
	if (gameData && gameData.bestRun)
		bestRun = gameData.bestRun;
	
	if (bestRun && bestRun[0]) {
	
		score_tf.text = ""+bestRun[0];
		level_tf.text = ""+bestRun[1];
		time_tf.text = ""+bestRun[2]
		combo_tf.text = ""+bestRun[3];
		lives_tf.text = ""+(bestRun[4]-1);
		
		weaponIcon.gotoAndStop(1+(bestRun[5]));
		gearIcon0.gotoAndStop(1+(bestRun[6]));
		gearIcon1.gotoAndStop(1+(bestRun[7]));
		gearIcon2.gotoAndStop(1+(bestRun[8]));
		gearIcon3.gotoAndStop(1+(bestRun[9]));
		
	} else {
		gotoAndStop(2);
	}
}

function update():void {
	
}]]></script>
