var ready:Boolean;
var go:Boolean;
var keyFilter0:Boolean;

var sndCtrl:MovieClip;
var _parent:MovieClip;
var gameData:MovieClip;
var ctrl:PlayerControl;

function init ():void {
	visible = true;
	x = 0;
	y = 0;
	
	sndCtrl = MovieClip(root);
	_parent = MovieClip(parent);
	gameData = MovieClip(root);
	ctrl = MovieClip(root).player1Control;
	
	_parent.instruct1.visible = false;
	_parent.instruct2.visible = false;
	
	ready = true;
	go = true;	
	
	container.init();
}

function update ():void {	
	if (go) {
		
		container.update();
		
		//ctrl back
		if (ctrl.ctrl_cancel) {
			if (!keyFilter0) {
				sndCtrl.playUISound(sndCtrl.sndButton2);
				_parent.closeAchieveMenu();
				go = false;
				
				keyFilter0 = true;
			}
		} else {
			keyFilter0 = false;
		}
	}
}]]></script>
