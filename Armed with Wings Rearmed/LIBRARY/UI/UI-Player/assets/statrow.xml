              <Actionscript>
                <script><![CDATA[import flash.display.MovieClip;

function update(stat:int, currStat:int):void {
	for (var i:int = 0; i < 20; i ++) {
		var clip:MovieClip = this["stat"+i];
		if (i < stat) {
			if (i >= currStat && MovieClip(root).gameMode != "MULTIPLAYER") clip.gotoAndStop(4);
			else clip.gotoAndStop(2);
		} else {
			if (i < currStat  && MovieClip(root).gameMode != "MULTIPLAYER") clip.gotoAndStop(3);
			else clip.gotoAndStop(1);
		}
	}
}

function updateIndex(stat:int, currStat:int, i:int):void {
	
		var clip:MovieClip = this["stat"+i];
		if (i < stat) {
			if (i >= currStat && MovieClip(root).gameMode != "MULTIPLAYER") clip.gotoAndStop(4);
			else clip.gotoAndStop(2);
		} else {
			if (i < currStat  && MovieClip(root).gameMode != "MULTIPLAYER") clip.gotoAndStop(3);
			else clip.gotoAndStop(1);
		}
	
}]]></script>
