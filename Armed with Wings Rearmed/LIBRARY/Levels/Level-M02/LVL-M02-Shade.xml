                <script><![CDATA[import flash.display.MovieClip;

var eagle:MovieClip;
var isActive:Boolean;

init();

function init():void {
	eagle = MovieClip(parent).eagle;
}

function update():void {
	if (eagle.bound.hitTestObject(this)) {
		isActive = true;
	} else {
		isActive = false;
	}
	
	if (isActive) {
		if (alpha > 0) alpha -= .05;
	} else {
		if (alpha < 1) alpha += .05;
	}
}]]></script>
