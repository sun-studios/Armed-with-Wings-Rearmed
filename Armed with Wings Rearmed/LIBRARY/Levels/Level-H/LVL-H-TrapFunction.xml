<DOMSymbolItem xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ns.adobe.com/xfl/2008/" name="Levels/Level-H/LVL-H-TrapFunction" itemID="54b94eaa-00000b9a" lastModified="1446212655">
  <timeline>
    <DOMTimeline name="LVL-H-TrapFunction">
      <layers>
        <DOMLayer name="actions" color="#9933CC" current="true" isSelected="true" autoNamed="false">
          <frames>
            <DOMFrame index="0" keyMode="9728">
              <Actionscript>
                <script><![CDATA[import flash.display.MovieClip;
import flash.events.Event;

visible = false;

var hero:MovieClip;
var trapDoor:MovieClip;
var ground:MovieClip;

init();

function  init ():void {
	hero = MovieClip(parent).hero;
	trapDoor = MovieClip(parent).ground.trapDoor;
	ground = MovieClip(parent).ground;
}

function  update ():void {
	if (hero.x > (trapDoor.x + ground.x+70)) {
		openTrap();
	}
}

function openTrap():void {
	if (trapDoor.currentFrame == 1) {
		hero.xSpeed *= .2;
		trapDoor.gotoAndPlay(2);
		MovieClip(parent).soundFader.on = true;
	}
}]]></script>
              </Actionscript>
              <elements/>
            </DOMFrame>
          </frames>
